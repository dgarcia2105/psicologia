<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <script src="https://kit.fontawesome.com/3baae96af7.js" crossorigin="anonymous"></script> @*DANY*@
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-expand-sm bg-light navbar-light">
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a class="nav-link" href="~/Home/Index">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="~/Usuario/Index">Usuarios</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="~/Usuario/AgregarUsuario">Agregar Usuario</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="~/Home/Contact">Contacto</a>
            </li>
            <li class="nav-item">
                <a class="nav-link disabled" href="~/Home/About">Acerca de</a>
            </li>
        </ul>
    </nav>
    <div class="container-fluid body-content ">
        <div class="">
            @RenderBody()
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Atención Profesional</p> @*Atención Profesional*@     @*Psicología Profesional CAAM*@
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
