@model MM.CAAM.Gestion.DTO.DTOs.ConsultaDTO

<div class="row">
    <div class="col-2"><span>@Model.Position </span>@Model.MotivoConsulta</div>
    <a href="~/Consulta/VerConsulta/?UsuarioId=@Com.Encryptor(Model.UsuarioId.ToString())&ConsultaId=@Com.Encryptor(Model.Id.ToString())">
        Ver consulta completa
    </a>
    <a href="~/Consulta/NuevaConsulta/?UsuarioId=@Com.Encryptor(Model.UsuarioId.ToString())&ConsultaId=@Com.Encryptor(Model.Id.ToString())">
        Editar
    </a>
</div>

